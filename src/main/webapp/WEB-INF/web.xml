<web-app>
  <display-name>Web Archive HelloWorld</display-name>
  
  <servlet>
  	<servlet-name>RandomMessageServlet</servlet-name>
  	<servlet-class>de.saschascherrer.code.blogroulette.servlets.RandomMessageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>RandomMessageServlet</servlet-name>
  	<url-pattern>/GetRandomMessage</url-pattern>
  </servlet-mapping>
</web-app>
